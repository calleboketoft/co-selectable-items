{
  "name": "@calle/ng2-selectable-items",
  "version": "1.0.2",
  "main": "ng2-selectable-items.js",
  "scripts": {
    "build": "typings install && npm run typescript && npm run sass",
    "install_webdriver": "node_modules/protractor/bin/webdriver-manager update",
    "sass": "gulp sass",
    "start": "node server.js",
    "test": "npm run test-unit && npm run test-e2e",
    "test-e2e": "npm run install_webdriver && npm run typescript && gulp test-e2e",
    "test-unit": "karma start",
    "typescript": "tsc",
    "watch": "tsc -w",
    "typings": "typings"
  },
  "peerDependencies": {
    "@angular/core": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@angular/core": "2.0.0-rc.5",
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "bootstrap": "git+https://git@github.com/twbs/bootstrap.git#v4.0.0-alpha",
    "es6-shim": "0.35.0",
    "express": "4.14.0",
    "ghooks": "1.3.2",
    "gulp": "3.9.1",
    "gulp-sass": "2.3.2",
    "jasmine-core": "2.4.1",
    "karma": "0.13.22",
    "karma-chrome-launcher": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-systemjs": "0.14.0",
    "protractor": "4.0.3",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.36",
    "typescript": "2.0.0",
    "typings": "1.3.2",
    "zone.js": "0.6.12"
  },
  "keywords": [
    "angular2"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/calleboketoft/ng2-selectable-items.git"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run build"
    }
  },
  "description": "Selectable items component for Angular 2"
}
