{
  "name": "co-selectable-items",
  "version": "0.2.1",
  "main": "src/co-selectable-items/co-selectable-items-cmp",
  "devDependencies": {
    "angular2": "^2.0.0-alpha.46",
    "bootstrap": "git+https://git@github.com/twbs/bootstrap.git#v4.0.0-alpha",
    "express": "^4.13.3",
    "ghooks": "^1.0.0",
    "gulp": "^3.9.0",
    "gulp-sass": "^2.1.0",
    "jasmine-core": "^2.3.4",
    "jspm": "^0.16.15",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-jasmine": "^0.3.6",
    "karma-jspm": "^2.0.2",
    "protractor": "^3.0.0",
    "typescript": "^1.6.2"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "scripts": {
    "build": "npm run tsc && npm run sass && npm run tsc-e2e",
    "postinstall": "jspm install && npm run webdriver",
    "test": "npm run test-unit && npm run test-e2e",
    "start": "node server.js",
    "prepublish": "npm run build",
    "sass": "gulp sass",
    "tsc": "tsc -p src",
    "watch-tsc": "tsc -p src -w",
    "test-unit": "karma start",
    "webdriver": "node_modules/protractor/bin/webdriver-manager update",
    "tsc-e2e": "tsc -p test/e2e",
    "watch-tsc-e2e": "tsc -p test/e2e -w",
    "test-e2e": "protractor protractor.conf.js"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run build && npm run test"
    }
  },
  "description": "Selectable items component for Angular 2",
  "jspm": {
    "configFile": "jspm.config.js",
    "devDependencies": {
      "angular2": "npm:angular2@^2.0.0-alpha.46",
      "reflect-metadata": "npm:reflect-metadata@^0.1.2",
      "zone.js": "npm:zone.js@^0.5.8",
      "typescript": "npm:typescript@^1.6.2"
    }
  }
}
