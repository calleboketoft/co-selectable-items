{
  "name": "@calle/ng2-selectable-items",
  "version": "1.0.10",
  "main": "index.js",
  "scripts": {
    "build": "npm run typescript && npm run install_webdriver",
    "install_webdriver": "node_modules/protractor/bin/webdriver-manager update",
    "start": "node server.js",
    "test": "npm run test-unit && npm run test-e2e",
    "test-e2e": "npm run install_webdriver && npm run typescript && gulp test-e2e",
    "test-unit": "karma start",
    "typescript": "tsc",
    "watch": "tsc -w"
  },
  "peerDependencies": {
    "@angular/core": "^2.2.3",
    "@angular/forms": "^2.2.3"
  },
  "devDependencies": {
    "@angular/common": "2.2.3",
    "@angular/compiler": "2.2.3",
    "@angular/core": "2.2.3",
    "@angular/forms": "2.2.3",
    "@angular/platform-browser": "2.2.3",
    "@angular/platform-browser-dynamic": "2.2.3",
    "@types/core-js": "0.9.34",
    "bootstrap": "4.0.0-alpha.5",
    "express": "4.14.0",
    "ghooks": "1.3.2",
    "gulp": "3.9.1",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.0.2",
    "karma-systemjs": "0.16.0",
    "protractor": "4.0.11",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.41",
    "tslint-config-standard": "2.0.0",
    "typescript": "2.0.10",
    "zone.js": "0.7.2"
  },
  "keywords": [
    "angular2"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/calleboketoft/ng2-selectable-items.git"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run build"
    }
  },
  "description": "Selectable items component for Angular 2"
}
