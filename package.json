{
  "name": "co-selectable-items",
  "version": "0.3.7",
  "main": "src/co-selectable-items/co-selectable-items-cmp",
  "scripts": {
    "build": "npm run typescript && npm run sass",
    "install_webdriver": "node_modules/protractor/bin/webdriver-manager update",
    "prepublish": "npm run build",
    "sass": "gulp sass",
    "start": "node server.js",
    "test": "npm run test-unit && npm run test-e2e",
    "test-e2e": "npm run install_webdriver && npm run typescript && gulp test-e2e",
    "test-unit": "karma start",
    "typescript": "gulp typescript",
    "watch": "gulp typescript:watch",
    "typings": "typings install"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.7",
    "es6-promise": "3.1.2",
    "es6-shim": "0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.23",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "bootstrap": "git+https://git@github.com/twbs/bootstrap.git#v4.0.0-alpha",
    "express": "4.13.4",
    "ghooks": "1.0.3",
    "gulp": "3.9.1",
    "gulp-sass": "2.2.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-typescript": "2.12.0",
    "jasmine-core": "2.4.1",
    "karma": "0.13.21",
    "karma-chrome-launcher": "0.2.2",
    "karma-jasmine": "0.3.7",
    "karma-systemjs": "0.12.0",
    "protractor": "3.1.1",
    "typescript": "1.8.2",
    "typings": "^0.6.8"
  },
  "keywords": [
    "angular2"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/calleboketoft/co-selectable-items.git"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run build"
    }
  },
  "description": "Selectable items component for Angular 2"
}
