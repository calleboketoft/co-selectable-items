{
  "name": "co-selectable-items",
  "version": "0.3.2",
  "main": "src/co-selectable-items/co-selectable-items-cmp",
  "dependencies": {
    "angular2": "^2.0.0-beta.0",
    "bootstrap": "git+https://git@github.com/twbs/bootstrap.git#v4.0.0-alpha",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.13",
    "express": "^4.13.3",
    "ghooks": "^1.0.0",
    "gulp": "^3.9.0",
    "gulp-sass": "^2.1.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-jasmine": "^0.3.6",
    "karma-systemjs": "^0.10.0",
    "protractor": "^3.0.0",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.0",
    "systemjs": "^0.19.9",
    "typescript": "^1.7.5",
    "zone.js": "^0.5.10"
  },
  "keywords": [
    "angular2"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/calleboketoft/co-selectable-items.git"
  },
  "scripts": {
    "build": "npm run tsc && npm run sass && npm run tsc-e2e",
    "postinstall": "npm run webdriver",
    "test": "npm run test-unit && npm run test-e2e",
    "start": "node server.js",
    "prepublish": "npm run build",
    "sass": "gulp sass",
    "tsc": "tsc -p src",
    "watch-tsc": "tsc -p src -w",
    "tsc-unit": "tsc -p test/unit",
    "test-unit": "karma start",
    "webdriver": "node_modules/protractor/bin/webdriver-manager update",
    "tsc-e2e": "tsc -p test/e2e",
    "watch-tsc-e2e": "tsc -p test/e2e -w",
    "test-e2e-directly": "protractor protractor.conf.js",
    "test-e2e": "npm run tsc-e2e && gulp test-e2e"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run build && npm run test-unit"
    }
  },
  "description": "Selectable items component for Angular 2"
}
